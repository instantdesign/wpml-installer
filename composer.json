{
  "name": "lucguerraz/wpml-installer",
  "description": "A composer plugin to make installing wpml.org plugins with composer easy. Your subscription info will be read from environment variables, will only be transmitted to WPML and will not be displayed in composer.lock.",
  "version": "1.0",
  "type": "composer-plugin",
  "keywords": [
    "wpml", "wordpress", "multilingual", "wp", "composer"
  ],
  "homepage": "https://github.com/lucguerraz/wpml-installer",
  "readme": "blob/main/README.md",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Luc Guerraz",
      "homepage": "https://github.com/lucguerraz",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/lucguerraz/wpml-installer/issues"
  },
  "require": {
    "php": ">=8.0",
    "composer-plugin-api": "^2.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "composer/composer": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "lucguerraz\\wpmlinstaller\\": "src/"
    }
  },
  "extra": {
    "class": "lucguerraz\\wpmlinstaller\\wpmlinstaller",
    "plugin-modifies-downloads": true
  }
}
